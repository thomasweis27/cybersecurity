#Hello, this is a test! 

def extract_dest_ip_lines(input_file_path, output_file_path):
  with open(input_file_path, 'r') as input_file, open(output_file_path, 'w') as output_file:
      lines = input_file.readlines()
      for i, line in enumerate(lines):
          if line.startswith('      "hostname": "10.47.8.20",'):
              #for j in range(i, min(i+7, len(lines))):  # Pull 6 lines down
              output_file.write(lines[i+7])

# Replace 'input_file.txt' and 'output_file.txt' with your actual file paths
extract_dest_ip_lines('file.txt', 'output_file.txt')
